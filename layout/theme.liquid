<!doctype html>
<!--[if IE 9]> <html class="ie9 no-js" lang="{{ shop.locale }}"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="no-js" lang="{{ shop.locale }}"> <!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <meta name="theme-color" content="{{ settings.color_button }}">
  <link rel="canonical" href="{{ canonical_url }}">

  {% if settings.favicon != blank %}
  <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
  {% endif %}

  {% capture seo_title %}
  {{ page_title }}
  {% if current_tags %}
  {%- assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags -}}
  {% endif %}
  {% if current_page != 1 %}
  &ndash; {{ 'general.meta.page' | t: page: current_page }}
  {% endif %}
  {% unless page_title contains shop.name %}
  &ndash; {{ shop.name }}
  {% endunless %}
  {% endcapture %}
  <title>{{ seo_title }}</title>

  {% if page_description %}
  <meta name="description" content="{{ page_description | escape }}">
  {% endif %}

  {% include 'social-meta-tags' %}
  <!--
  {{ 'theme.scss.css' | asset_url | stylesheet_tag }}
  {% include 'google-fonts' %}

  <script>
  var theme = {
  strings: {
  addToCart: {{ 'products.product.add_to_cart' | t | json }},
  soldOut: {{ 'products.product.sold_out' | t | json }},
  unavailable: {{ 'products.product.unavailable' | t | json }},
  showMore: {{ 'general.filters.show_more' | t | json }},
  showLess: {{ 'general.filters.show_less' | t | json }}
},
moneyFormat: {{ shop.money_format | json }}
}

document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
</script>
-->

{{ '//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css' | stylesheet_tag }}

{{ '//fonts.googleapis.com/css?family=Lato:100,300,400,700,400italic,700italic' | stylesheet_tag }}
{{ '//fonts.googleapis.com/css?family=Dancing+Script:400,700' | stylesheet_tag }}

<!-- Owl Stylesheets -->
{{ 'owl.carousel.min.css' | asset_url | stylesheet_tag }}
{{ 'owl.theme.default.min.css' | asset_url | stylesheet_tag }}

<!-- Plugin CSS -->
{{ '//maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css' | stylesheet_tag }}

<!-- Theme CSS -->
{{ 'resto.css' | asset_url | stylesheet_tag }}



{{ content_for_header }}
</head>

<body class="template-{{ template | split: '.' | first }} home">

{% section 'header' %}

  <section id="download" class="download bg-primary text-center">
    <div class="container">
      <div class="col-md-10 col-md-offset-1 slider">
        <div class="box">
          <div class="col-md-7 col-sm-7">
            <img src="{{ '1.jpg' | asset_url }}" alt="{{ shop.name }}" class="img-responsive">
          </div>
          <div class="col-md-5 col-sm-5">
            <div class="caption-first">
              <h3> Modern Filipino Style </h3>
              <p>Experience menu choices that reveal the character of Philippines, region by region, prepared with only the freshest ingredients by our highly skilled culinary team.</p>
            </div>
          </div>
        </div>
        <div class="box">
          <div class="col-md-5 col-sm-5">
            <div class="caption-first">
              <h3> Great Service! </h3>
              <p>Itâ€™s not just about first rate cuisine and an award winning wine list. Our highly trained and exceedingly personable staff are there to help you navigate our selections and ensure a memorable dining experience.</p>
            </div>
          </div>
          <div class="col-md-7 col-sm-7">
            <img src="{{ '2.jpg' | asset_url }}" alt="{{ shop.name }}" class="img-responsive second-food">
          </div>
        </div>
      </div>

    </div>
  </section>


  <section class="cta" id="specialties">
    <div class="parallax-layer parallax-layer-base">
      <div class="cta-content ">
        <div class="container text-center">
          <h2>Our Menus</h2>
        </div>
      </div>
      <div class="overlay"></div>
    </div>
  </section>

  {% section 'front-home' %}

  {% section 'newsletter' %}
  <!-- Bootstrap End -->
  {{content_for_layout}}
  {% section 'footer' %}

  <!-- javascript -->
  {{ 'jquery.min.js' | asset_url | script_tag }}
  {{ 'owl.carousel.js' | asset_url | script_tag }}

  {{ '//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js' | script_tag }}
  {{ '//cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js' | script_tag }}

  <!-- Theme JavaScript -->
  <!-- <script src="{{'new-age.min.js' | asset_url }}" defer="defer"></script>
  <script src="{{'jquery.parallax.min.js' | asset_url }}" defer="defer"></script> -->
  {{ 'new-age.min.js' | asset_url | script_tag }}
  {{ 'jquery.parallax.min.js' | asset_url | script_tag }}
  <script type="text/javascript">
  $('.cta').parallax("-100px", 0.4);
  // $('header').parallax("-100px", 0.1);
  </script>
  <script>

  $('.owl-carousel').owlCarousel({
    loop:true,
    margin:10,
    dots: false,
    nav:true,
    autoplay: true,
    responsive:{
      0:{
        items:1
      },
      600:{
        items:3
      },
      1000:{
        items:3
      }
    }
  });

  </script>
</body>
</html>
